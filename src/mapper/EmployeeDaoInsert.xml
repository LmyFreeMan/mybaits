<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
        "   http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--必须和它所描述的全类名保持一致-->
<mapper namespace="dao.EmployeeDao">
    <!--必须和它所描述的接口名称保持一致-->
<insert id="insertEmp"  >
    insert into test.employee(id,last_name,email,gender,did) values (#{id},#{last_name},#{email},#{gender},#{did})
</insert>
</mapper>        
